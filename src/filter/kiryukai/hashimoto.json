{
    "description": "Filter to hide insulting tweets about Japanese gymnasts posted with #桐生ココ",
    "rule": [
        "and",
        [
            {
                "mode": "include",
                "type": "hashtag",
                "string": "#桐生ココ"
            },
            [
                "or",
                [
                    {
                        "mode": "include",
                        "type": "hashtag",
                        "string": "#橋本大輝"
                    },
                    {
                        "mode": "include",
                        "type": "hashtag",
                        "string": "#橋本大辉"
                    }
                ]
            ]
        ]
    ]
}