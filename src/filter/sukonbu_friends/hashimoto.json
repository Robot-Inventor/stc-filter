{
    "description": "Filter to hide insulting tweets about Japanese gymnasts posted with #白上フブキ.",
    "rule": [
        "and",
        [
            {
                "mode": "include",
                "type": "hashtag",
                "string": "#白上フブキ"
            },
            [
                "or",
                [
                    {
                        "mode": "include",
                        "type": "hashtag",
                        "string": "#橋本大輝"
                    },
                    {
                        "mode": "include",
                        "type": "hashtag",
                        "string": "#橋本大辉"
                    }
                ]
            ]
        ],
        {
            "default": "Olympic-related trolling",
            "ja": "オリンピック関連の荒らし"
        }
    ]
}